{
  "name": "rosettaWizard",
  "description": "Enhances your rosetta stone performances",
  "version": "3.0.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "./dist/worker.esm.js"
  },
  "action": {
    "default_popup": "/static/index.html",
    "default_icon": "/static/rosettaWizard-48.png",
    "default_title": "rosetta Wizard"
  },
  "permissions": ["storage", "webRequest", "tabs", "scripting"],
  "icons": {
    "48": "/static/rosettaWizard-48.png",
    "512": "/static/rosettaWizard-512.png"
  },
  "host_permissions": [
    "*://totale.rosettastone.com/",
    "*://tracking.rosettastone.com/",
    "*://learn.rosettastone.com/",
    "*://gaia-server.rosettastone.com/"
  ]
}
